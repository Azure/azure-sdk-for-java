          "justification": "skip check for third party files."
        },
        {
          "regex": true,
          "code": "java\\.method\\.addedToInterface",
          "new": "method .* com\\.azure\\.resourcemanager\\..*",
          "justification": "resourcemanager interfaces are allowed to add methods."
        },
        {
          "regex": true,
          "code": "java\\.method\\.addedToInterface",
