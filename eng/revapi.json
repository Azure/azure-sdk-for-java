[
    {
        "extension": "revapi.java",
        "configuration": {
            "filter": {
                "packages": {
                    "regex": true,
                    "exclude" : ["com\\.azure\\..+\\.implementation(\\..+)?"]
                }
            }
        }
    },
    {
        "extension": "revapi.semver.ignore",
        "configuration": {
          "enabled" : true,
          "versionIncreaseAllows" : {
            "major" : "breaking",
            "minor" : "nonBreaking",
            "patch" : "equivalent"
          },
          "passThroughDifferences": ["java.class.nonPublicPartOfAPI"]
        }
      }
]
