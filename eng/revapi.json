[
    {
        "extension": "revapi.filter",
        "configuration": {
            "elements": {
                "include" : ["class com\\.azure\\..*"],
                "exclude" : ["class com\\.azure\\..*\\.implementation(\\..*)?"]
            }
        }
    },
    {
        "extension": "revapi.semver.ignore",
        "configuration": {
          "enabled" : true,
          "versionIncreaseAllows" : {
            "major" : "breaking",
            "minor" : "nonBreaking",
            "patch" : "equivalent"
          },
          "passThroughDifferences": ["java.class.nonPublicPartOfAPI"]
        }
      }
]
