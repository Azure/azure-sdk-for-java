[
  {
    "extension": "revapi.java",
    "configuration": {
      "missing-classes": {
        "behavior": "ignore",
        "ignoreMissingAnnotations": false
      },
      "matchOverloads": true
    }
  },
  {
    "extension": "revapi.versions",
    "configuration": {
      "enabled": true,
      "semantic0": false,
      "versionIncreaseAllows": {
        "major": {
          "severity": "BREAKING"
        },
        "minor": {
          "severity": "NON_BREAKING"
        },
        "patch": {
          "severity": "EQUIVALENT"
        }
      },
      "onAllowed": {
        "remove": true,
        "attachments": {
          "breaksVersioningRules": "false"
        }
      },
      "onDisallowed": {
        "criticality": "error",
        "attachments": {
          "breaksVersioningRules": "true"
        }
      },
      "passThroughDifferences": [
        "java.class.nonPublicPartOfAPI"
      ]
    }
  },
  {
