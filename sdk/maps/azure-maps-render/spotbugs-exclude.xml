<?xml version="1.0" encoding="UTF-8"?>

<FindBugsFilter xmlns="https://github.com/spotbugs/filter/3.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                xsi:schemaLocation="https://github.com/spotbugs/filter/3.0.0 https://raw.githubusercontent.com/spotbugs/spotbugs/3.1.0/spotbugs/etc/findbugsfilter.xsd">
  <Match>
    <Bug pattern="EI_EXPOSE_REP" />
    <Or>
      <Class name="com.azure.maps.render.implementation.models.BoundingBox" />
      <Class name="com.azure.maps.render.models.Copyright" />
      <Class name="com.azure.maps.render.models.ErrorDetail" />
      <Class name="com.azure.maps.render.models.MapAttribution" />
      <Class name="com.azure.maps.render.models.MapStaticImageOptions" />
      <Class name="com.azure.maps.render.models.MapTileset" />
      <Class name="com.azure.maps.render.models.RegionCopyrights" />
    </Or>
  </Match>
  <Match>
    <Bug pattern="EI_EXPOSE_REP2" />
    <Or>
      <Class name="com.azure.maps.render.implementation.RenderClientImplBuilder" />
      <Class name="com.azure.maps.render.implementation.models.BoundingBox" />
      <Class name="com.azure.maps.render.models.MapAttribution" />
      <Class name="com.azure.maps.render.models.MapStaticImageOptions" />
      <Class name="com.azure.maps.render.models.MapTileset" />
    </Or>
  </Match>
  <Match>
    <Bug pattern="UWF_FIELD_NOT_INITIALIZED_IN_CONSTRUCTOR" />
    <Class name="com.azure.maps.render.models.MapTileset" />
  </Match>
</FindBugsFilter>
