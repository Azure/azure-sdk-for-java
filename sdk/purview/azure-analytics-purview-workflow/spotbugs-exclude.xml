      <Class name="com.azure.analytics.purview.workflow.ApprovalClientBuilder" />
      <Class name="com.azure.analytics.purview.workflow.TaskStatusClientBuilder" />
      <Class name="com.azure.analytics.purview.workflow.UserRequestsClientBuilder" />
      <Class name="com.azure.analytics.purview.workflow.WorkflowClientBuilder" />
      <Class name="com.azure.analytics.purview.workflow.WorkflowRunClientBuilder" />
      <Class name="com.azure.analytics.purview.workflow.WorkflowRunsClientBuilder" />
      <Class name="com.azure.analytics.purview.workflow.WorkflowTaskClientBuilder" />
      <Class name="com.azure.analytics.purview.workflow.WorkflowTasksClientBuilder" />
      <Class name="com.azure.analytics.purview.workflow.WorkflowsClientBuilder" />
    </Or>
  </Match>
</FindBugsFilter>
