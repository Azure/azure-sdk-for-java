<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd">

  <modelVersion>4.0.0</modelVersion>

  <parent>
    <groupId>com.azure.spring</groupId>
    <artifactId>azure-spring-boot-test-parent</artifactId>
    <version>1.0.0</version> <!-- {x-version-update;com.azure.spring:azure-spring-boot-test-parent;current} -->
    <relativePath>../azure-spring-boot-test-parent</relativePath>
  </parent>

  <artifactId>azure-spring-boot-test-servicebus-jms-standard</artifactId>
  <version>1.0.0</version> <!-- {x-version-update;com.azure.spring:azure-spring-boot-test-servicebus-jms;current} -->

  <dependencies>
    <dependency>
      <groupId>com.azure.spring</groupId>
      <artifactId>azure-spring-boot-starter-servicebus-jms</artifactId>
      <version>3.2.0-beta.1</version> <!-- {x-version-update;com.azure.spring:azure-spring-boot-starter-servicebus-jms;current} -->
    </dependency>

    <dependency>
      <groupId>org.springframework.boot</groupId>
      <artifactId>spring-boot-starter-test</artifactId>
      <scope>test</scope>
      <exclusions>
        <exclusion>
          <groupId>org.junit.vintage</groupId>
          <artifactId>junit-vintage-engine</artifactId>
        </exclusion>
      </exclusions>
    </dependency>
  </dependencies>

  <build>
    <plugins>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-surefire-plugin</artifactId>
        <version>3.0.0-M3</version> <!-- {x-version-update;org.apache.maven.plugins:maven-surefire-plugin;external_dependency} -->
        <configuration>
          <systemPropertyVariables>
            <SPRING_JMS_SERVICEBUS_CONNECTION_STRING>${env.SPRING_JMS_STANDARD_SERVICEBUS_CONNECTION_STRING}</SPRING_JMS_SERVICEBUS_CONNECTION_STRING>
            <SPRING_JMS_SERVICEBUS_PRICING_TIER>${env.SPRING_JMS_STANDARD_SERVICEBUS_PRICING_TIER}</SPRING_JMS_SERVICEBUS_PRICING_TIER>
          </systemPropertyVariables>
        </configuration>
      </plugin>
    </plugins>
  </build>
</project>
