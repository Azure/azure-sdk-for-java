      "location": "[variables('location')]",
      "dependsOn": [
        "[resourceId('Microsoft.ServiceBus/namespaces', variables('azureServiceBusNamespaceName_1'))]"
      ],
      "properties": {
        "defaultMessageTimeToLive": "P14D",
