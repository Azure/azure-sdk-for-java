# ConfigData Location Resolvers
 org.springframework.boot.context.config.ConfigDataLocationResolver=\
 com.azure.spring.cloud.appconfiguration.config.implementation.AzureAppConfigDataLocationResolver,  com.azure.spring.cloud.appconfiguration.config.implementation.FeatureManagementLocationResolver

 # ConfigData Loaders
 org.springframework.boot.context.config.ConfigDataLoader=\
com.azure.spring.cloud.appconfiguration.config.implementation.FeatureManagementDataLoader, com.azure.spring.cloud.appconfiguration.config.implementation.AzureAppConfigDataLoader
