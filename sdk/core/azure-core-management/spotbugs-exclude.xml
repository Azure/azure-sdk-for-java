<?xml version="1.0" encoding="UTF-8"?>

<FindBugsFilter xmlns="https://github.com/spotbugs/filter/3.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="https://github.com/spotbugs/filter/3.0.0 https://raw.githubusercontent.com/spotbugs/spotbugs/3.1.0/spotbugs/etc/findbugsfilter.xsd">
  <Match>
    <Bug pattern="EI_EXPOSE_REP" />
    <Or>
      <And>
        <Class name="com.azure.core.management.AzureEnvironment" />
        <Method name="getEndpoints" />
      </And>
      <And>
        <Class name="com.azure.core.management.Resource" />
        <Method name="tags" />
      </And>
      <And>
        <Class name="com.azure.core.management.exception.ManagementError" />
        <Or>
          <Method name="getAdditionalInfo" />
          <Method name="getDetails" />
        </Or>
      </And>
      <And>
        <Class name="com.azure.core.management.polling.PollResult$Error" />
        <Method name="getResponseHeaders" />
      </And>
    </Or>
  </Match>

  <Match>
    <Bug pattern="EI_EXPOSE_REP2" />
    <Or>
      <And>
        <Or>
          <Class name="com.azure.core.management.AzureEnvironment" />
          <Class name="com.azure.core.management.polling.PollResult$Error" />
        </Or>
        <Method name="&lt;init&gt;" />
      </And>
      <And>
        <Class name="com.azure.core.management.Resource" />
        <Method name="withTags" />
      </And>
    </Or>
  </Match>

  <Match>
    <Bug pattern="EI_EXPOSE_STATIC_REP2" />
    <Class name="com.azure.core.management.implementation.ProxyResourceAccessHelper" />
    <Method name="setAccessor" />
  </Match>
</FindBugsFilter>
