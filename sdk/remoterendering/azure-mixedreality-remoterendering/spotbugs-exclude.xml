<?xml version="1.0" encoding="UTF-8"?>

<FindBugsFilter xmlns="https://github.com/spotbugs/filter/3.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                xsi:schemaLocation="https://github.com/spotbugs/filter/3.0.0 https://raw.githubusercontent.com/spotbugs/spotbugs/3.1.0/spotbugs/etc/findbugsfilter.xsd">
  <Match>
    <Bug pattern="EI_EXPOSE_REP" />
    <Or>
      <Class name="com.azure.mixedreality.remoterendering.implementation.models.ConversionList" />
      <Class name="com.azure.mixedreality.remoterendering.implementation.models.Error" />
      <Class name="com.azure.mixedreality.remoterendering.implementation.models.SessionsList" />
      <Class name="com.azure.mixedreality.remoterendering.models.RemoteRenderingServiceError" />
    </Or>
  </Match>
  <Match>
    <Bug pattern="EI_EXPOSE_REP2" />
    <Or>
      <Class name="com.azure.mixedreality.remoterendering.implementation.MixedRealityRemoteRenderingImplBuilder" />
      <Class name="com.azure.mixedreality.remoterendering.implementation.models.ConversionList" />
      <Class name="com.azure.mixedreality.remoterendering.implementation.models.SessionsList" />
      <Class name="com.azure.mixedreality.remoterendering.models.RemoteRenderingServiceError" />
    </Or>
  </Match>
  <Match>
    <Bug pattern="URF_UNREAD_FIELD" />
    <Class name="com.azure.mixedreality.remoterendering.RemoteRenderingClientBuilder" />
  </Match>
  <Match>
    <Bug pattern="UWF_FIELD_NOT_INITIALIZED_IN_CONSTRUCTOR" />
    <Class name="com.azure.mixedreality.remoterendering.RemoteRenderingClientBuilder" />
  </Match>
</FindBugsFilter>
