      <scope>provided</scope>
    </dependency>

    <dependency>
      <groupId>com.azure</groupId>
      <artifactId>azure-storage-file-share</artifactId>
      <version>12.13.0</version> <!-- {x-version-update;com.azure:azure-storage-file-share;dependency} -->
      <scope>provided</scope>
    </dependency>

    <dependency>
