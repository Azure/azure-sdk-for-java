      <artifactId>spring-cloud-azure-resourcemanager</artifactId>
      <version>4.1.0-beta.1</version> <!-- {x-version-update;unreleased_com.azure.spring:spring-cloud-azure-resourcemanager;dependency} -->
      <scope>provided</scope>
