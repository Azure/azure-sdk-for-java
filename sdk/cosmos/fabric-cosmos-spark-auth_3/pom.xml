            <executions>
              <execution>
                <id>02-shade</id>
                <phase>package</phase>
                <goals>
                  <goal>shade</goal>
                </goals>
                <configuration>
                  <relocations>
                    <relocation>
                      <pattern>com.azure</pattern>
                      <shadedPattern>${shadingPrefix}.com.azure</shadedPattern>
                      <excludes>
                        <exclude>com.azure.cosmos.spark.*</exclude>
