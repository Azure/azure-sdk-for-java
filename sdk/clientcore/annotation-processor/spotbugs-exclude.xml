<?xml version="1.0" encoding="UTF-8"?>

<FindBugsFilter xmlns="https://github.com/spotbugs/filter/3.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                xsi:schemaLocation="https://github.com/spotbugs/filter/3.0.0 https://raw.githubusercontent.com/spotbugs/spotbugs/3.1.0/spotbugs/etc/findbugsfilter.xsd">
  <Match>
    <Bug pattern="DCN_NULLPOINTER_EXCEPTION" />
    <Class name="io.clientcore.annotation.processor.utils.PathBuilderTest" />
  </Match>
  <Match>
    <Bug pattern="DLS_DEAD_LOCAL_STORE" />
    <Class name="io.clientcore.annotation.processor.utils.PathBuilderTest" />
  </Match>
  <Match>
    <Bug pattern="DM_CONVERT_CASE" />
    <Class name="io.clientcore.annotation.processor.templating.JavaPoetTemplateProcessor" />
  </Match>
  <Match>
    <Bug pattern="DM_DEFAULT_ENCODING" />
    <Class name="io.clientcore.annotation.processor.templating.BodyContentTypeProcessorTest" />
  </Match>
  <Match>
    <Bug pattern="NM_CONFUSING" />
    <Class name="io.clientcore.annotation.processor.templating.APIGenerationTest" />
  </Match>
  <Match>
    <Bug pattern="ST_WRITE_TO_STATIC_FROM_INSTANCE_METHOD" />
    <Class name="io.clientcore.annotation.processor.templating.JavaPoetTemplateProcessor" />
  </Match>
  <Match>
    <Bug pattern="UWF_FIELD_NOT_INITIALIZED_IN_CONSTRUCTOR" />
    <Or>
      <Class name="io.clientcore.annotation.processor.templating.APIGenerationTest" />
      <Class name="io.clientcore.annotation.processor.templating.HttpPipelineBuilderMethodTest" />
      <Class name="io.clientcore.annotation.processor.templating.JavaPoetTemplateProcessor" />
    </Or>
  </Match>
</FindBugsFilter>
