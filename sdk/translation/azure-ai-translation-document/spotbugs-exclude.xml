<?xml version="1.0" encoding="UTF-8"?>

<FindBugsFilter xmlns="https://github.com/spotbugs/filter/3.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                xsi:schemaLocation="https://github.com/spotbugs/filter/3.0.0 https://raw.githubusercontent.com/spotbugs/spotbugs/3.1.0/spotbugs/etc/findbugsfilter.xsd">
  <Match>
    <Bug pattern="EI_EXPOSE_REP" />
    <Or>
      <Class name="com.azure.ai.translation.document.implementation.MultipartFormDataHelper" />
      <Class name="com.azure.ai.translation.document.implementation.models.SupportedFileFormats" />
      <Class name="com.azure.ai.translation.document.models.DocumentFileDetails" />
      <Class name="com.azure.ai.translation.document.models.DocumentTranslateContent" />
      <Class name="com.azure.ai.translation.document.models.DocumentTranslationInput" />
      <Class name="com.azure.ai.translation.document.models.FileFormat" />
      <Class name="com.azure.ai.translation.document.models.GlossaryFileDetails" />
      <Class name="com.azure.ai.translation.document.models.TranslationBatch" />
      <Class name="com.azure.ai.translation.document.models.TranslationTarget" />
    </Or>
  </Match>
  <Match>
    <Bug pattern="EI_EXPOSE_REP2" />
    <Or>
      <Class name="com.azure.ai.translation.document.DocumentTranslationClientBuilder" />
      <Class name="com.azure.ai.translation.document.SingleDocumentTranslationClientBuilder" />
      <Class name="com.azure.ai.translation.document.models.DocumentFileDetails" />
      <Class name="com.azure.ai.translation.document.models.DocumentTranslateContent" />
      <Class name="com.azure.ai.translation.document.models.DocumentTranslationInput" />
      <Class name="com.azure.ai.translation.document.models.GlossaryFileDetails" />
      <Class name="com.azure.ai.translation.document.models.TranslationBatch" />
      <Class name="com.azure.ai.translation.document.models.TranslationTarget" />
    </Or>
  </Match>
</FindBugsFilter>
