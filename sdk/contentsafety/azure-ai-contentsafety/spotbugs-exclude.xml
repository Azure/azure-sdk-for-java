<?xml version="1.0" encoding="UTF-8"?>

<FindBugsFilter xmlns="https://github.com/spotbugs/filter/3.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                xsi:schemaLocation="https://github.com/spotbugs/filter/3.0.0 https://raw.githubusercontent.com/spotbugs/spotbugs/3.1.0/spotbugs/etc/findbugsfilter.xsd">
  <Match>
    <Bug pattern="EI_EXPOSE_REP" />
    <Or>
      <Class name="com.azure.ai.contentsafety.models.AddOrUpdateTextBlocklistItemsOptions" />
      <Class name="com.azure.ai.contentsafety.models.AddOrUpdateTextBlocklistItemsResult" />
      <Class name="com.azure.ai.contentsafety.models.AnalyzeImageOptions" />
      <Class name="com.azure.ai.contentsafety.models.AnalyzeImageResult" />
      <Class name="com.azure.ai.contentsafety.models.AnalyzeTextOptions" />
      <Class name="com.azure.ai.contentsafety.models.AnalyzeTextResult" />
      <Class name="com.azure.ai.contentsafety.models.RemoveTextBlocklistItemsOptions" />
    </Or>
  </Match>
  <Match>
    <Bug pattern="EI_EXPOSE_REP2" />
    <Or>
      <Class name="com.azure.ai.contentsafety.BlocklistClientBuilder" />
      <Class name="com.azure.ai.contentsafety.ContentSafetyClientBuilder" />
      <Class name="com.azure.ai.contentsafety.models.AddOrUpdateTextBlocklistItemsOptions" />
      <Class name="com.azure.ai.contentsafety.models.AnalyzeImageOptions" />
      <Class name="com.azure.ai.contentsafety.models.AnalyzeTextOptions" />
      <Class name="com.azure.ai.contentsafety.models.RemoveTextBlocklistItemsOptions" />
    </Or>
  </Match>
</FindBugsFilter>
