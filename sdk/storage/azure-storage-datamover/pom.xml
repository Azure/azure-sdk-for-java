    </javaModulesSurefireArgLine>

    <!-- No docs now -->
    <doclint>all,-missing</doclint>
  </properties>

  <dependencies>
    <dependency>
      <groupId>com.azure</groupId>
      <artifactId>azure-storage-common</artifactId>
