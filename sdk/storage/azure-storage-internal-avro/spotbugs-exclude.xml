<?xml version="1.0" encoding="UTF-8"?>

<FindBugsFilter xmlns="https://github.com/spotbugs/filter/3.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                xsi:schemaLocation="https://github.com/spotbugs/filter/3.0.0 https://raw.githubusercontent.com/spotbugs/spotbugs/3.1.0/spotbugs/etc/findbugsfilter.xsd">
  <Match>
    <Bug pattern="BC_UNCONFIRMED_CAST" />
    <Class name="com.azure.storage.internal.avro.implementation.schema.AvroSchema" />
  </Match>
  <Match>
    <Bug pattern="EI_EXPOSE_REP2" />
    <Or>
      <Class name="com.azure.storage.internal.avro.implementation.schema.AvroSchema" />
      <Class name="com.azure.storage.internal.avro.implementation.schema.complex.AvroEnumSchema" />
      <Class name="com.azure.storage.internal.avro.implementation.schema.complex.AvroRecordSchema" />
      <Class name="com.azure.storage.internal.avro.implementation.schema.complex.AvroUnionSchema" />
    </Or>
  </Match>
</FindBugsFilter>
