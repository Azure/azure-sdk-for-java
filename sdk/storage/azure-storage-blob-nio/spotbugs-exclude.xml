<?xml version="1.0" encoding="UTF-8"?>

<FindBugsFilter xmlns="https://github.com/spotbugs/filter/3.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                xsi:schemaLocation="https://github.com/spotbugs/filter/3.0.0 https://raw.githubusercontent.com/spotbugs/spotbugs/3.1.0/spotbugs/etc/findbugsfilter.xsd">
  <Match>
    <Bug pattern="CT_CONSTRUCTOR_THROW" />
    <Class name="com.azure.storage.blob.nio.AzureDirectoryStream$AzureDirectoryIterator" />
  </Match>
  <Match>
    <Bug pattern="EI_EXPOSE_REP" />
    <Or>
      <Class name="com.azure.storage.blob.nio.AzureDirectoryStream" />
      <Class name="com.azure.storage.blob.nio.AzureFileSystem" />
    </Or>
  </Match>
  <Match>
    <Bug pattern="NP_BOOLEAN_RETURN_NULL" />
    <Class name="com.azure.storage.blob.nio.AzureBlobFileAttributes" />
  </Match>
</FindBugsFilter>
