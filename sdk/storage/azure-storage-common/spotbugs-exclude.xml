<?xml version="1.0" encoding="UTF-8"?>

<FindBugsFilter xmlns="https://github.com/spotbugs/filter/3.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                xsi:schemaLocation="https://github.com/spotbugs/filter/3.0.0 https://raw.githubusercontent.com/spotbugs/spotbugs/3.1.0/spotbugs/etc/findbugsfilter.xsd">
  <Match>
    <Bug pattern="BC_UNCONFIRMED_CAST_OF_RETURN_VALUE" />
    <Class name="com.azure.storage.common.implementation.FluxInputStream" />
  </Match>
  <Match>
    <Bug pattern="CT_CONSTRUCTOR_THROW" />
    <Class name="com.azure.storage.common.StorageInputStream" />
  </Match>
  <Match>
    <Bug pattern="EI_EXPOSE_REP" />
    <Class name="com.azure.storage.common.implementation.StorageSeekableByteChannel" />
  </Match>
  <Match>
    <Bug pattern="EI_EXPOSE_REP2" />
    <Class name="com.azure.storage.common.implementation.ByteBufferMarkableInputStream" />
  </Match>
  <Match>
    <Bug pattern="NP_NULL_ON_SOME_PATH_FROM_RETURN_VALUE" />
    <Class name="com.azure.storage.common.policy.RequestRetryPolicy" />
  </Match>
  <Match>
    <Bug pattern="BC_UNCONFIRMED_CAST" />
    <Class name="com.azure.storage.common.implementation.StorageImplUtils" />
  </Match>
</FindBugsFilter>
